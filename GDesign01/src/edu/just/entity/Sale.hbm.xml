<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.just.entity">

    <class name="Sale" table="Sale">
        <id name="id" column="SALE_ID">
            <generator class="native"/>
        </id>
        <property name="name" type="string" column="SALE_NAME"/>
        <property name="profit" type="int" column="SALE_PROFIT"/>
        <property name="month" type="string" column="SALE_MONTH"/>
        
    </class>

</hibernate-mapping>
